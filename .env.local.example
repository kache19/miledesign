# Gemini API Key for AI Consultant functionality
# Get your API key from: https://aistudio.google.com/app/apikey
GEMINI_API_KEY=your_gemini_api_key_here

# Base path for built assets:
# - Use "/" for Docker/Nginx and most servers
# - Use "/miledesign/" for GitHub Pages repo hosting
VITE_APP_BASE=/

# Security: disable browser-side admin in production by default
# Set true only in trusted/private deployments
VITE_ENABLE_ADMIN=false

# Supabase project settings
VITE_SUPABASE_URL=https://YOUR_PROJECT_ID.supabase.co
VITE_SUPABASE_ANON_KEY=YOUR_SUPABASE_ANON_KEY

# Inquiry email recipient used by the contact form
VITE_INQUIRY_EMAIL=info@miledesigns.com

# Default inquiry WhatsApp number used by the contact form and floating button
VITE_INQUIRY_WHATSAPP_NUMBER=+18880000000
